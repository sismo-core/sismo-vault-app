{
  "name": "sismo-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@sentry/react": "^7.10.0",
    "@sismo-core/crypto": "^1.0.6",
    "@sismo-core/hydra-s1": "2.0.0-beta4",
    "@sismo-core/pythia-1": "^1.0.6",
    "@sismo-core/zk-connect-client": "^0.0.0-beta.1",
    "@synaps-io/react-verify": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.13",
    "@web3-onboard/core": "^2.2.9",
    "@web3-onboard/injected-wallets": "^2.0.7",
    "@web3-onboard/ledger": "^2.1.0",
    "@web3-onboard/trezor": "^2.1.0",
    "@web3-onboard/walletconnect": "^2.0.1",
    "axios": "^0.27.2",
    "canvas-confetti": "^1.5.1",
    "crypto-js": "^4.1.1",
    "ethers": "^5.5.4",
    "fuzzyset": "^1.0.7",
    "graphql": "^16.6.0",
    "idb": "^7.1.1",
    "lodash.debounce": "^4.0.8",
    "lottie-web": "^5.9.4",
    "luxon": "^3.2.1",
    "phosphor-react": "^1.4.1",
    "react": "^17.0.2",
    "react-blockies": "^1.4.1",
    "react-dom": "^17.0.2",
    "react-lazy-load-image-component": "^1.5.6",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "readline": "^1.3.0",
    "sha3": "^2.1.4",
    "styled-components": "^5.3.3",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "export REACT_APP_GIT_HASH=$(git rev-parse HEAD) && PORT=3000 GENERATE_SOURCEMAP=false react-app-rewired start",
    "build": "export REACT_APP_GIT_HASH=$(git rev-parse HEAD) && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "prepare": "husky install",
    "eslint": "eslint src --max-warnings=0",
    "delete-maps": "yarn run delete-map-files && yarn run delete-references-to-map-files",
    "delete-map-files": "find ./build -name '*.map' -delete",
    "delete-references-to-map-files": "find ./build -regex '.*\\.\\(js\\|css\\)' -exec sed -i -E '\\/[\\*\\/]#\\ssourceMappingURL=main(\\.[0-9a-f]+)?\\.(css|js)\\.map(\\*\\/)?/g' {} +"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.24",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.10.0",
    "https-browserify": "^1.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "os-browserify": "^0.3.0",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "util": "^0.12.4"
  },
  "engines": {
    "npm": "USE YARN INSTEAD OF NPM"
  },
  "lint-staged": {
    "src/**/*": "prettier --write --ignore-unknown",
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint src --max-warnings=0"
    ]
  }
}
